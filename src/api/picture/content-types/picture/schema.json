{
  "kind": "collectionType",
  "collectionName": "pictures",
  "info": {
    "singularName": "picture",
    "pluralName": "pictures",
    "displayName": "Картины",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "history": {
      "type": "richtext",
      "required": true
    },
    "autor": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::autor.autor",
      "mappedBy": "pictures"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "picture"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    }
  }
}
